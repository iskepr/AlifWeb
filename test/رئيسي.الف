
استورد الوقت
س = الوقت.الان()
الوقت.غفوة(3)
ش = الوقت.الان()
اطبع(ش - س)
س = الوقت.وقت()
اطبع(س)
اطبع(م"التاريخ: {س["السنة"]}-{س["الشهر"]}-{س["اليوم"]} \سالوقت: {س["الساعة"]}:{س["الدقيقة"]}:{س["الثانية"]}")
س = الوقت.منسق()
اطبع(س)

س = "هذا نص"
اطبع(س)
س = م"هذا نص منسق حيث 9 + 7 = {9 + 7}"
اطبع(س)
س = "السلام " + "عليكم"
اطبع(س)
س = "سطر\س\سطر جديد"
س = "سطر\nسطر جديد"
س = "سطر\سسطر جديد"
اطبع(س)
