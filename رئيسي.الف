ص = [2,"3",8,2, ادخل("قم بإدخال نص:") ,10,صح, 9]
اطبع(ص)
نص(اقصى(ص),"color: green;")
اطبع(ادنى(ص))
رابط("موقع الف","https://aliflang.org/","color: black;")

