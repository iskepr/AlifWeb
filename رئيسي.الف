اطبع("نص هنا او متغير")
نص("السلام عليكم","color: red;")
س = "هذا نص عادي"
س = {"أ": 5, "ب": 7, "ت": 9}
س = مصفوفة('{"الاسم":"محمد", "العمر":19}')
س = [5, "اهلا", صح, 2]
س = (5, "اهلا", صح, 2)
س = 4
عام س
نص(س)
اطبع({"الاسم":صح, "العمر":19})
اطبع(طول(س))
صحيح("1")
عشري("0.5")
مصفوفة('{"الاسم":"صح", "العمر":19}')  # دعم كلمة صح وخطا وعدم [ ]
# ارجع محمد
# استمر
